<h1>Listing autos</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Year</th>
      <th>Make</th>
      <th>Model</th>
      <th>Category</th>
      <th>Car company</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @autos.each do |auto| %>
      <tr>
        <td><%= auto.user_id %></td>
        <td><%= auto.year %></td>
        <td><%= auto.make %></td>
        <td><%= auto.model %></td>
        <td><%= auto.category %></td>
        <td><%= auto.car_company_id %></td>
        <td><%= link_to 'Show', auto %></td>
        <td><%= link_to 'Edit', edit_auto_path(auto) %></td>
        <td><%= link_to 'Destroy', auto, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Auto', new_auto_path %>
